import os
from ete2 import Tree

def getfnames():
    result = []
    tree = Tree()
    for root, dirs, fnames in os.walk('.'):
        result.extend([f for f in fnames if fname.endswith('.java')])
    return result


def buildtree(fnames_arg):
    fnames = fnames_arg[:]

    for f in fnames:
    with open(fname, 'r') as f:
        fname = f.read()


def run():
    buildtree(getfnames())


if __name__ == '__main__':
    run()
